{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "amd": true,
    "worker": true
  },
  "extends": "eslint:recommended",
  "plugins": ["vue"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "indent": ["warn", 2, { "SwitchCase": 1 }],
    "semi": ["error", "always"]
  }
}
